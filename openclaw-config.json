{
  "agents": {
    "defaults": {
      "model": {
        "primary": "anthropic/claude-sonnet-4-5"
      }
    }
  },
  "gateway": {
    "port": 10000,
    "bind": "lan",
    "verbose": true,
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "channels": {
    "whatsapp": {
      "dmPolicy": "pairing"
    }
  },
  "workspace": {
    "path": "/root/.openclaw/workspace"
  }
}
